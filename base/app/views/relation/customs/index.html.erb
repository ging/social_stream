<% toolbar :profile, :subject => current_subject, :option => 'contacts' %>

<div id="relation_customs">   
  <h2><%= t 'relation_custom.title' %></h2>

    <%= render :partial => 'relation/customs/list', :object => @customs %>

    <div id="permissions">
      <% @customs.each do |r| %>
        <div id="relation_<%= r.id %>_permissions" class="relation_permissions">
          <div class="loading"></div>
        </div>
      <% end %>
    </div>

    <%= javascript_tag do %>
      $(function() {
        SocialStream.RelationCustom.index();
      })
    <% end %>
</div> 
