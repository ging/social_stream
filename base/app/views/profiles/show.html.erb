<% content_for :title do%>
  <%= @profile.subject.name %>
<% end %>

<% if @profile.description? %>
  <% content_for :description do %>
    <%= @profile.description %>
  <% end %>
<% end %>

<% content_for :keywords do %>
  <%= @profile.subject.tag_list%>
<% end %>

<% sidebar %>

<% toolbar :profile, :subject => @profile.subject %>

<%= render :partial => "profile" %>
